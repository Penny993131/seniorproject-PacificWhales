@model IEnumerable<Harmony.Models.User>

@if (Model.Count() > 0)
{
    <div class="row">
        <div class="col-10">
            <table>
                <tr>
                    <th>Name</th>
                    <th>Location</th>
                    <tth>Email</tth>
                    <th></th>
                </tr>
                @foreach (var user in Model)
                {
                <tr>
                    <td>@Html.DisplayFor(x => user.FirstName) @Html.DisplayFor(x => user.LastName)</td>
                    <td>@Html.DisplayFor(x => user.City) @Html.DisplayFor(x => user.State)</td>
                    <td>@Html.DisplayFor(x => user.Email)</td>
                    <td>@Html.ActionLink("Profile", "MusicianDetails", "Users", new { id = user.ID }, null)</td>
                </tr>

                }
            </table>
        </div>
    </div>
}
@if (Model.Count() == 0)
{
    <h2 style="text-align:center;">No Matches</h2>
}

@Html.ActionLink("Search Again", "Search")