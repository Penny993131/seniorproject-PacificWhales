<!-- Search form -->
    <div class="form-group row">
        <form class="form-inline md-form form-sm active-cyan-2 mt-2">
            <input class="form-control form-control-sm mr-3 w-75" type="text" placeholder="Search"
                   aria-label="Search">

            <div class="form-check">
                <input class="form-check-input" type="radio" name="searchOption" id="musician" value="option1" checked>
                <label class="form-check-label" for="musician">Musicians</label>
            </div>
            <div class="form-check">
                <input class="form-check-input" type="radio" name="searchOption" id="venue" value="option2">
                <label class="form-check-label" for="venue">Venues</label>
            </div>
            <button type="submit" class="btn btn-primary">Search</button>
        </form>
    </div>
    